; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Twin Shooter Starting Kit"
config/tags=PackedStringArray("starterkit")
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3")
config/icon="res://project_icon.png"

[autoload]

DebugStats="*res://tools/debugging/DebugStats.tscn"
DebugOverlay="*res://tools/debugging/DebugOverlay.tscn"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"dr_megadroon_character": "res://characters/NPC/dr_megadroon_character.dch"
}
directories/dtl_directory={
"dr_megadroon": "res://characters/NPC/dr_megadroon.dtl",
"level1_drmegadroon": "res://scenes/level1/level1_drmegadroon.dtl",
"level1_intro": "res://scenes/level1/level1_intro.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://ui/dialogic_style/dialog_style.tres"]
layout/default_style="res://ui/dialogic_style/dialog_style.tres"
variables={
"status": "starting"
}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.01
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
text/input_action="p1_interact"
text/autoadvance_enabled=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/always_on_top=true
window/size/window_width_override=960
window/size/window_height_override=540

[editor_plugins]

enabled=PackedStringArray("res://addons/beehave/plugin.cfg", "res://addons/dialogic/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "orange",
"res://characters/": "red",
"res://scenes/": "blue",
"res://tools/": "yellow",
"res://ui/": "green"
}

[filesystem]

import/blender/enabled=false

[global]

window=false
wind=false
wint=false

[global_group]

bullet="group for all bullets"

