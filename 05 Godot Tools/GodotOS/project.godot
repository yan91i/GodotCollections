; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotOS"
config/description="A fake operating system interface, created by popcar2 for fun!"
config/version="1.1.0"
run/main_scene="res://Scenes/Main/MainScene.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="GodotOS"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/low_processor_mode=true
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/image="res://Art/Godot Logo.png"
config/icon="res://icon.svg"

[autoload]

HotkeyManager="res://Scenes/Autoloads/Hotkey Manager/hotkey_manager.tscn"
ContextMenu="*res://Scenes/Autoloads/Context Menu/context_menu.tscn"
NotificationManager="*res://Scenes/Autoloads/Notification Manager/notification_manager.tscn"
CopyPasteManager="*res://Scenes/Autoloads/CopyPaste Manager/copy_paste_manager.tscn"
GlobalValues="*res://Scenes/Autoloads/GlobalValues/global_values.tscn"
DefaultValues="*res://Scenes/Autoloads/Default Values/default_values.tscn"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/vsync/vsync_mode=0

[file_customization]

folder_colors={
"res://Art/": "green",
"res://Default Files/": "gray",
"res://Games/": "red",
"res://Scenes/": "blue",
"res://Scenes/Misc/": "gray"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/default_font_hinting=2
theme/default_font_multichannel_signed_distance_field=true
theme/lcd_subpixel_layout=0

